		<script type="text/javascript">
			var cover = document.getElementById("cover");
			var cnt = 0;
			cover.addEventListener("click", function() {
				if(cnt++ %2==1){
					cover.scr="../images/cup-1.png";
				}else{
					cover.src="../images/cup-2.png";
				}
				
				cover.src="../images/cup-2.png";
			})
		</script>
